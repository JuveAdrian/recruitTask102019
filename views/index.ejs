            <div id="page-wrapper">
                    <h1>10 most polluted cities</h1>
                <form action="/form-data" method="POST">

                    <label for="default">Type country: Poland, Germany, Spain or France</label>
                    <input type="text" id="default" name="nameCountry" list="languages" placeholder="Type here...">
                    
                    <datalist id="languages">
                        <option value="Poland">
                        <option value="Germany">
                        <option value="Spain">
                        <option value="France">
                    </datalist>
                    <button type="submit">Submit</button>
                </form>    
                
            </div>

               
